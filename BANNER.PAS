Function StrToUpper(S:String):String;
Var
 I:Byte;
Begin
 For I:=1 to Length(S)do Begin
  If S[I] in['a'..'z']Then S[I]:=Chr(Ord(S[I])-32);
 End;
 StrToUpper:=S;
End;

Var
 Language:(_French,_English,_Germany,_Italian,_Spain);
 OutputData:(odNone,odBash,odC,odPascal);
 Font:(_Default,_Computer,_Script);
 DefaultChar:Char;
 OutputFileName,TmpLanguage,TmpChr:String;
 FileOutput:Text;
 ValueChar:Byte;
 P,I,J,K:Integer;
 Masque,Err:Word;
 CurrString:String;
 TB:Array[0..Height-1,0..79]of Char;

BEGIN
 Language:=_French;
 TmpLanguage:=GetEnv('LANGUAGE');
 If TmpLanguage<>''Then Begin
  If TmpLanguage[1]='"'Then TmpLanguage:=Copy(TmpLanguage,2,255);
  If StrToUpper(Copy(TmpLanguage,1,2))='EN'Then Language:=_English Else
  If StrToUpper(Copy(TmpLanguage,1,2))='GR'Then Language:=_Germany Else
  If StrToUpper(Copy(TmpLanguage,1,2))='IT'Then Language:=_Italian Else
  If StrToUpper(Copy(TmpLanguage,1,2))='SP'Then Language:=_Spain;
 End;
 If(ParamStr(1)='/?')or(ParamStr(1)='--help')or(ParamStr(1)='-h')Then Begin
  Case Language of
   _English:Begin
    WriteLn('Print the ASCII character string in large letter to standard output.');
    WriteLn;
    WriteLn('Syntax: BANNER text [/OUTPUT:format] [/FILE:file]');
   End;
   Else Begin
    WriteLn('BANNER : Cette commande permet d''afficher une bannire.');
    WriteLn;
    WriteLn('Syntaxe : BANNER message [/OUTPUT:format] [/FILE:fichier] [/CHAR:valeur] [/FONT:name]');
    WriteLn;
    WriteLn(' message        Message  sortir');
    WriteLn('/CHAR:valeur    Un caractre ou une valeur entre 0 et 255. Par dfaut #.');
    WriteLn('/FILE:fichier   Nom du fichier de sortie  place de l''cran');
    WriteLn('/FONT:nom       Nom de la police : DEFAULT, COMPUTER, SCRIPT,...');
    WriteLn('/OUTPUT:BASH    Message en code source BASH');
    WriteLn('/OUTPUT:C       Message en code source C');
    WriteLn('/OUTPUT:PASCAL  Message en code source Pascal');
   End;
  End;
 End
  Else
 If ParamCount>0Then Begin
  Font:=_Default;
  DefaultChar:='#';
  OutputData:=odNone;
  OutputFileName:='';
  For P:=1to ParamCount do Begin
   If(StrToUpper(Copy(ParamStr(P),1,Length('/CHAR:')))='/CHAR:')Then Begin
    TmpChr:=Copy(ParamStr(P),Length('/CHAR:')+1,255);
    If Length(TmpChr)=1 Then DefaultChar:=TmpChr[1]
     Else
    Begin
     Val(TmpChr,ValueChar,Err);
     If Err=0 Then DefaultChar:=Char(ValueChar);
    End;
   End
    Else
   If(StrToUpper(Copy(ParamStr(P),1,Length('/FILE:')))='/FILE:')Then Begin
    OutputFileName:=Copy(ParamStr(P),Length('/FILE:')+1,255);
   End
    Else
   If(StrToUpper(Copy(ParamStr(P),1,Length('/FONT:')))='/FONT:')Then Begin
    TmpChr:=Copy(ParamStr(P),Length('/FONT:')+1,255);
    If StrToUpper(TmpChr)='DEFAULT'Then Font:=_Default Else
    If StrToUpper(TmpChr)='COMPUTER'Then Font:=_Computer Else
    If StrToUpper(TmpChr)='SCRIPT'Then Font:=_Script;
   End
    Else
   If StrToUpper(ParamStr(P))='/OUTPUT:BASH'Then Begin
    OutputData:=odBASH;
   End
    Else
   If StrToUpper(ParamStr(P))='/OUTPUT:C'Then Begin
    OutputData:=odC;
   End
    Else
   If StrToUpper(ParamStr(P))='/OUTPUT:PASCAL'Then Begin
    OutputData:=odPascal;
   End;
  End;
  For P:=1to ParamCount do Begin
   CurrString:=ParamStr(P);
   If(StrToUpper(Copy(ParamStr(P),1,Length('/CHAR:')))='/CHAR:')Then Begin
    { Saut ... }
   End
    Else
   If(StrToUpper(Copy(ParamStr(P),1,Length('/FILE:')))='/FILE:')Then Begin
    { Saut ... }
   End
    Else
   If(StrToUpper(Copy(ParamStr(P),1,Length('/FONT:')))='/FONT:')Then Begin
    { Saut ... }
   End
    Else
   If StrToUpper(ParamStr(P))='/OUTPUT:BASH'Then Begin
    { Saut ... }
   End
    Else
   If StrToUpper(CurrString)='/OUTPUT:C'Then Begin
    { Saut ...}
   End
    Else
   If StrToUpper(CurrString)='/OUTPUT:PASCAL'Then Begin
    { Saut ...}
   End
    Else
   Begin
    { Gnration de la chaine de caractres }
    If Length(CurrString)>10Then CurrString:=Copy(CurrString,1,10);
    FillChar(TB,SizeOf(TB),0);
    For J:=0to Length(CurrString)-1do For I:=0to Height-1 do Begin
     Case Font of
      _Computer:Masque:=FontCOMPUTER[Height*Byte(CurrString[J+1])+I];
      _Script:Masque:=FontSCRIPT[Height*Byte(CurrString[J+1])+I];
      Else Masque:=Matrix8x16[Height*Byte(CurrString[J+1])+I];
     End;
     For K:=0to 7 do Begin
      If Masque and 128<>0Then TB[I,J*8+K]:=DefaultChar
                          Else TB[I,J*8+K]:=' ';
      Masque:=Masque shl 1;
     End;
    End;
     { Sortie de la chaine de caractres }
    If OutputFileName<>''Then Begin
     {$I-}Assign(FileOutput,OutputFileName);
     Rewrite(FileOutput);{$I+}
     If IOResult<>0 Then Begin
      WriteLn('Impossible de crer le fichier de sortie ',OutputFileName);
      Halt(1);
     End;
     Case(OutputData)of
      odC:WriteLn(FileOutput,'void main() {');
      odPascal:WriteLn(FileOutput,'BEGIN');
     End;
     For J:=0to Height-1do Begin
      Case(OutputData)of
       odBash:Write(FileOutput,'echo "');
       odC:Write(FileOutput,'printf(''');
       odPascal:Write(FileOutput,'WriteLn(''');
      End;
      For I:=0to Length(CurrString)*8-1do Write(FileOutput,TB[J,I]);
      Case(OutputData)of
       odBASH:Write(FileOutput,'\n";');
       odC:Write(FileOutput,'\n");');
       odPascal:Write(FileOutput,''');');
      End;
      WriteLn(FileOutput);
     End;
     Case(OutputData)of
      odC:WriteLn(FileOutput,'}');
      odPascal:WriteLn(FileOutput,'END.');
     End;
     Close(FileOutput);
    End
     Else
    Begin
     Case(OutputData)of
      odC:WriteLn('void main() {');
      odPascal:WriteLn('BEGIN');
     End;
     For J:=0to Height-1do Begin
      Case(OutputData)of
       odBash:Write('echo "');
       odC:Write('printf(''');
       odPascal:Write('WriteLn(''');
      End;
      For I:=0to Length(CurrString)*8-1do Write(TB[J,I]);
      Case(OutputData)of
       odBASH:Write('\n";');
       odC:Write('\n");');
       odPascal:Write(''');');
      End;
      WriteLn;
     End;
     Case(OutputData)of
      odC:WriteLn('}');
      odPascal:WriteLn('END.');
     End;
    End;
   End;
  End;
 End;
END.
